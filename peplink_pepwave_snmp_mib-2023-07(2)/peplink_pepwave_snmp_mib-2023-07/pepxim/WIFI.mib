            wifiApWlanConfigBroadcastSsid OBJECT-TYPE
                SYNTAX INTEGER {
                        disable(0),
                        enable(1)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "Wi-Fi AP WLAN broadcast SSID status."
                ::= { wifiApWlanConfigEntry 4 }

            wifiApWlanConfigVlanId OBJECT-TYPE
                SYNTAX INTEGER(0..4094)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
