-- *  Table
-- **********************************************************************
        wifiApWlanConfigTable OBJECT-TYPE
            SYNTAX SEQUENCE OF WifiApWlanConfigEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "Wi-Fi AP WLAN config table"
            ::= { wifiApWlanConfig 1 }

            wifiApWlanConfigEntry OBJECT-TYPE
            SYNTAX WifiApWlanConfigEntry
            MAX-ACCESS not-accessible
            STATUS current
            DESCRIPTION
                "An entry in the wifiApWlanConfigTable"
            INDEX { wifiApWlanId }
            ::= { wifiApWlanConfigTable 1 }

            WifiApWlanConfigEntry ::=
            SEQUENCE {
                wifiApWlanConfigRowControl
                    RowStatus,
                wifiApWlanConfigSsid
                    OCTET STRING,
                wifiApWlanConfigSsidEnable
                    INTEGER,
                wifiApWlanConfigBroadcastSsid
                    INTEGER,
                wifiApWlanConfigVlanId
                    INTEGER,
                wifiApWlanConfigVlanPoolEnable
                    INTEGER,
                wifiApWlanConfigVlanPoolList
                    OCTET STRING
            }

            wifiApWlanConfigRowControl OBJECT-TYPE
                SYNTAX RowStatus
                MAX-ACCESS read-create
                STATUS current
                DESCRIPTION
