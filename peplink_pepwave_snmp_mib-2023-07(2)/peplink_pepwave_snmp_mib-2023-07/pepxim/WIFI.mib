-- *  Table
-- **********************************************************************
        wifiApWlanConfigSecurityTable OBJECT-TYPE
                SYNTAX SEQUENCE OF WifiApWlanConfigSecurityEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION
                    "Wi-Fi AP WLAN config security table"
                ::= { wifiApWlanConfig 2 }

            wifiApWlanConfigSecurityEntry OBJECT-TYPE
                SYNTAX WifiApWlanConfigSecurityEntry
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION
                    "An entry in the wifiApWlanConfigSecurityTable."
                    INDEX { wifiApWlanId }
                ::= { wifiApWlanConfigSecurityTable 1 }

            WifiApWlanConfigSecurityEntry ::=
            SEQUENCE {
                wifiApWlanConfigSecurityPolicy
                    INTEGER,
                wifiApWlanConfigPassphrase
                    OCTET STRING
             }

            wifiApWlanConfigSecurityPolicy OBJECT-TYPE
                SYNTAX INTEGER {
                        open(0),
                        wpa2Psk(1),
                        wpaOrWpa2Psk(2),
                        wpa3Psk(3),
                        wpa2OrWpa3Psk(4),
                        wpa2(5),
                        wpaOrWpa2(6),
                        owe(7),
                        wpa3(8)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "This attribute indicates the security level of the wireless network."
                ::= { wifiApWlanConfigSecurityEntry 1 }

            wifiApWlanConfigPassphrase OBJECT-TYPE
                SYNTAX OCTET STRING(SIZE(8..64))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "This attribute indicates the passphrase of the wireless network.
                    Remarks:
                    8 to 63 characters or 64 hex characters."
                ::= { wifiApWlanConfigSecurityEntry 2 }

