            wifiApWlanConfigVlanPoolEnable OBJECT-TYPE
                SYNTAX INTEGER {
                        disable(0),
                        enable(1)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "Wi-Fi AP WLAN vlan pool enable."
                ::= { wifiApWlanConfigEntry 6 }

            wifiApWlanConfigVlanPoolList OBJECT-TYPE
