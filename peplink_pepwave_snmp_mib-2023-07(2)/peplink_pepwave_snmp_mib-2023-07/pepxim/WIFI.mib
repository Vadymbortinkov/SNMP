                DESCRIPTION
                    "This attribute indicates the channel number to be
                    used. The default value is auto(0)."
                ::= { wifiRadioConfigEntry 2 }

            wifiApConfigPowerBoost OBJECT-TYPE
                SYNTAX INTEGER {
                    disable(0),
                    enable(1)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "This attribute indicates the power boost."
                ::= { wifiRadioConfigEntry 3 }

            wifiApConfigTxPower OBJECT-TYPE
                SYNTAX INTEGER {
                    max(1),
                    high(2),
                    medium(3),
                    low(4),
                    custom(0)
                }
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "This attribute indicates the transmit output power."
                ::= { wifiRadioConfigEntry 4 }

            wifiApConfigCustomTxPower OBJECT-TYPE
                SYNTAX Integer32
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                    "This attribute indicates the custom transmit output power [3..30] (units:dBm).
                    Remarks:
                    If the value equals 0, means custom transmit output power not applicable."
